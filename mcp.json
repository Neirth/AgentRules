{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-chrome-devtools"
      ]
    },
    "docker": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/docker"
      ]
    },
    "expo": {
      "command": "npx",
      "args": [
        "-y",
        "@expo/mcp-server"
      ]
    }
  }
}
